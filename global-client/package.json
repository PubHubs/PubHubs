{
	"name": "@pubhubs/global-client",
	"version": "0.5.0",
	"type": "module",
	"private": true,
	"scripts": {
		"lint": "eslint --ext .js,.vue,.ts --ignore-path .gitignore --fix src",
		"pwa": "vite build && npx http-server dist",
		"test": "vitest",
		"test:run": "vitest run",
		"test:ui": "vitest --ui",
		"coverage": "vitest run --coverage"
	},
	"engines": {
		"npm": ">=9.8.1",
		"node": ">=22.0.0"
	},
	"dependencies": {
		"@privacybydesign/yivi-client": "^0",
		"@privacybydesign/yivi-core": "^0",
		"@privacybydesign/yivi-web": "^0",
		"@vitejs/plugin-vue": "^5.2.1",
		"@vue/test-utils": "^2.2.3",
		"chai": "^5.2.1",
		"core-js": "^3.8.3",
		"mavon-editor": "^3.0.2",
		"pinia": "^3",
		"register-service-worker": "^1.7.2",
		"tailwind-merge": "^2",
		"typescript-cookie": "^1.0.4",
		"util": "^0.12.5",
		"vue": "^3.2.13",
		"vue-i18n": "^11",
		"vue-router": "^4.1.5",
		"vuedraggable": "^4",
		"vuejs3-logger": "1.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.12.16",
		"@babel/eslint-parser": "^7.12.16",
		"@tailwindcss/forms": "^0.5.10",
		"@trivago/prettier-plugin-sort-imports": "^5.2.2",
		"@typescript-eslint/eslint-plugin": "^7",
		"@typescript-eslint/parser": "^7",
		"@vitejs/plugin-vue": "^5",
		"@vue/eslint-config-typescript": "^13",
		"autoprefixer": "^10.4.21",
		"eslint": "^8",
		"eslint-plugin-vue": "^9",
		"fluid-tailwind": "^1.0.4",
		"jsdoc": "^4.0.2",
		"jsdom": "^26",
		"msw": "2.3.2",
		"postcss": "^8.5.3",
		"prettier": "^3.5.0",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.0.2",
		"vite-plugin-node-polyfills": "^0.23.0",
		"vite-plugin-pwa": "^1.0.0",
		"vitest": "^3.1.1",
		"vue-eslint-parser": "^9"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true,
			"vue/setup-compiler-macros": true
		},
		"extends": [
			"plugin:vue/vue3-essential",
			"eslint:recommended",
			"@vue/typescript"
		],
		"parserOptions": {
			"parser": "@typescript-eslint/parser"
		},
		"rules": {
			"no-unused-vars": "off",
			"@typescript-eslint/no-unused-vars": [
				"error",
				{
					"vars": "all",
					"args": "after-used",
					"varsIgnorePattern": "^props|value|Hub",
					"ignoreRestSiblings": false
				}
			],
			"vue/multi-word-component-names": "off",
			"no-mixed-spaces-and-tabs": [
				"warn",
				"smart-tabs"
			]
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not dead",
		"not ie 11"
	],
	"_id": "@",
	"readme": "ERROR: No README data found!"
}
